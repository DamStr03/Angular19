{
  "singleQuote": false,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "tabWidth": 4,
  "useTabs": true,
  "trailingComma": "all",
  "bracketSpacing": false,
  "jsxBracketSameLine": true,
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "src/**/*.html",
      "options": {
        "printWidth": 140
      }
    }
  ]
}
